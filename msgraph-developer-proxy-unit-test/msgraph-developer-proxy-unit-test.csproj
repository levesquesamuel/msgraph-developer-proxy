    <RootNamespace>msgraph_developer_proxy_unit_test</RootNamespace>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <UserSecretsId>420128d7-6842-455f-8531-01589d2a42d3</UserSecretsId>
